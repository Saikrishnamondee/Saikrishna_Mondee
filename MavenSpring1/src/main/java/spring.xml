<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


<bean id="swift" class="com.mondee.Car" >
<property name="fuelType" value="petrol"></property>
<property name="speed" value="170"></property>
</bean>
<bean id="orange" class="com.mondee.Bus" >
<property name="fuelType" value="Diesel"></property>
<property name="speed" value="140"></property>
</bean>
<bean id="royalenfield" class="com.mondee.Bike" >
<property name="fuelType" value="Petrol"></property>
<property name="speed" value="120"></property>
</bean>

 <bean id="goa" class="com.mondee.Travel" >
 <property name="v">
 <ref bean="royalenfield"/>
 </property>
 </bean>
</beans>